<main>
	<div class="container">
		<div class="row">
			<div class="col s12 m9 l10">
				<h4>Ajouter un concert</h4>
			</div>
				<%= form_for @tag do |f|%>
					<div class ="input-field col s12 m12 l12">
						<%= f.label :name %>
						<%= f.text_field :name %>
					</div>
					<div class="row">
						<div class ="input-field col s6 m6 l6">
						<%= f.submit "Ajouter", class: "btn"%>
						</div>
					</div>
				<% end %>
		</div>
	</div>
</main>

