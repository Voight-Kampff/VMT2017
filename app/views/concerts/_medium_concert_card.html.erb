<div class="concert-card">
	<div class="col s12 m12">
		<div class="card horizontal hoverable">
			<div class="card-image">
				<% unless concert.image.medium.url.nil? %>
					<%= image_tag concert.image.medium.url%>
				<% end %>
			</div>
			<div class="card-stacked">
				<%= render partial: "card_content", locals: {concert: concert, counter: counter}%>
			</div>
		</div>
	</div>
</div>